{
    "manifest_version": 3,
    "name": "Utility Toolkit",
    "description": "This extension is intended to assist users with daily browser activities and modify the user's experience.",
    "version": "1.0.0",
    "icons": {
      "128": "Icon_128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "Icon1.png"
      }
    },
    "permissions": [
      "alarms",
      "contextMenus",
      "system.cpu",
      "system.memory",
      "power",
      "desktopCapture",
      "geolocation",
      "system.display",
      "system.storage",
      "storage",
      "webRequest"
    ]
}
  